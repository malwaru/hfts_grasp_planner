<?xml version="1.0" encoding="utf-8"?>
<robot name="kuka-r850-sdh">
 <robot file="kuka-kr5-r850.zae" ></robot>
 <robot prefix="hand_" file="../schunk-sdh/schunk-sdh.zae"></robot>
 <kinbody>
  <body name="cylinder_link" type="dynamic">
    <offsetfrom>link6</offsetfrom>
    <translation>0.57 0.0 0.79</translation>
    <rotationaxis>0 0 1 -90</rotationaxis>
    <Geom type="cylinder">
      <radius>0.03</radius>
      <height>0.04</height>
      <diffuseColor>0.4 0.4 0.4</diffuseColor>
    </Geom>
  </body>
   <body name="hand_root">
     <offsetfrom>cylinder_link</offsetfrom>
     <translation>0.0 0.07 0.0</translation>
    <rotationaxis>1 0 0 -90</rotationaxis>
    <rotationaxis>0 1 0 300</rotationaxis>
     <!-- <translation>0.56 0.0 0.79</translation>  -->
     <!-- <rotationaxis>0 1 0 90</rotationaxis> -->
   </body>
   <joint name="dummy0" type="hinge" enable="false">
     <body>link6</body>
     <body>cylinder_link</body>
     <limits>0 0</limits>
   </joint>
   <joint name="dummy1" type="hinge" enable="false">
     <body>cylinder_link</body>
     <body>hand_root</body>
     <limits>0 0</limits>
   </joint>
 </kinbody>
<!-- <manipulator name="hand">
    <base>base</base>
    <effector>hand_root</effector>
    <joints>dummy0 dummy1</joints>
    <Translation>0 0 0</Translation>
</manipulator> -->
<manipulator name="arm_with_sdh">
    <base>base</base>
    <effector>hand_root</effector>
    <joints>hand_A1_Joint hand_A2_Joint hand_A3_Joint hand_B2_Joint hand_B3_Joint hand_C2_Joint hand_C3_Joint</joints>
    <closingdirection>1 1 1 1 1 1 1</closingdirection>
    <direction>0 0 1</direction>
</manipulator>
</robot>
